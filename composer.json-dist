                    "type": "git",
                    "reference": "master"
                }
            }
        }
    ],
    "require": {
        "pear-pear/Mail_Mime": ">=1.8.1",
        "pear-pear/Mail_mimeDecode": ">=1.5.5",
        "Net_SMTP": "dev-master",
        "pear-pear/Net_IDNA2": ">=0.1.1",
        "pear-pear/Auth_SASL": ">=1.0.6"
    },
    "minimum-stability": "dev"
