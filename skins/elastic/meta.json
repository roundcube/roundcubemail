		"apple-mobile-web-app-title": "Roundcube",
		"apple-mobile-web-app-capable": "yes",
		"msapplication-navbutton-color": "#f4f4f4"
	},
	"links": {
		"apple-touch-icon": "/images/apple-touch-icon.png",
		"manifest": "/manifest.json"
	}
